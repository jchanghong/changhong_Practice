<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="changhong_Practice.changhong"
          android:versionCode="1"
          android:versionName="1.0">
    <uses-sdk android:minSdkVersion="8"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.SET_WALLPAPER"/>
    <application android:label="@string/app_name"
                 android:theme="@style/CustomActionBarTheme"
                 android:icon="@drawable/ic_empty"
                 android:hardwareAccelerated="true"
                 android:name="changhong_Practice.config.Application"
            >

        <activity android:name="changhong_Practice.ui.WelcomeActivity">


        </activity>
        <activity android:name="changhong_Practice.ui.HomeActivity" android:launchMode="singleTask">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="changhong_Practice.ui.ImageAlubumGirdActivity"
                  android:launchMode="singleTask"/>
        <activity android:name="changhong_Practice.ui.SingleImagePagerActivity" android:theme="@style/over"/>
        <activity android:name="changhong_Practice.ui.SettingActivity" android:launchMode="singleTask"
                  android:excludeFromRecents="true"/>
        <activity android:name="changhong_Practice.ui.setting.SetfilesActivity" android:launchMode="singleTask"
                  android:excludeFromRecents="true"/>
        <activity android:name="changhong_Practice.ui.CropActivity"/>

    </application>
</manifest>
